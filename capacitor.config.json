{
  "appId": "com.solucnet.tecnicos",
  "appName": "SolucNet TÃ©cnicos",
  "webDir": "public",
  "android": {
    "buildOptions": {
      "keystorePath": "android/release.keystore",
      "keystoreAlias": "solucnet-key"
    },
    "allowMixedContent": true,
    "backgroundColor": "#28a745"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#28a745",
      "showSpinner": true,
      "androidSpinnerStyle": "large",
      "spinnerColor": "#ffffff"
